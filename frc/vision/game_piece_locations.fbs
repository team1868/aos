namespace frc.vision;

// Bounding box detection.
table GamePieceLocation {
  // Class ID of this detection
  class_id:uint8 (id: 0);

  // 0-1 confidence.
  confidence:float (id: 1);

  // Location of center of box in meters.
  x:float (id: 2);
  y:float (id: 3);

  // Size of the box in meters.
  width:float (id: 4);
  height:float (id: 5);
}

table GamePieceLocations {
  locations:[GamePieceLocation] (id: 0);
}

root_type GamePieceLocations;
