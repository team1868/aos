package(default_visibility = ["//visibility:public"])

cc_library(
    name = "unavailable",
    target_compatible_with = ["@platforms//:incompatible"],
)

alias(
    name = "gobject",
    actual = select({
        "@platforms//cpu:arm64": "@arm64_debian_sysroot//:gobject-2.0",
        "@platforms//cpu:x86_64": "@amd64_debian_sysroot//:gobject-2.0",
        "//conditions:default": ":unavailable",
    }),
)
